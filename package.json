{
  "name": "typescript-functional-programming",
  "version": "1.0.0",
  "description": "A high-performance functional programming library written in TypeScript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "dev": "ts-node-dev --respawn --transpile-only src/examples/index.ts",
    "clean": "rimraf dist",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write src/**/*.ts",
    "benchmark": "ts-node src/benchmarks/index.ts",
    "benchmark:compact": "ts-node src/arrays/compact-benchmark.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hun756/Javascript-Functional-Programming.git"
  },
  "keywords": [
    "functional-programming",
    "typescript",
    "performance",
    "utilities",
    "collection",
    "array",
    "object"
  ],
  "author": "Mehmet Şükrü Ekemen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hun756/Javascript-Functional-Programming/issues"
  },
  "homepage": "https://github.com/hun756/Javascript-Functional-Programming#readme",
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "^20.6.3",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {},
  "engines": {
    "node": ">=16.0.0"
  }
}
